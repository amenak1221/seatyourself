<h1>Add your wonderful restaurant below</h1>

<%= form_tag "/restaurants" do %>
<p>
  <%= label_tag :name %><br />
  <%= text_field_tag :name %>
</p>

<p>
  <%= label_tag :address %>
  <%= text_field_tag :address %>
</p>

<p>
  <%= label_tag :cuisine %><br />
  <%= text_field_tag :cuisine %>
</p>

<p>
  <%= label_tag :capacity %><br />
  <%= text_field_tag :capacity %><br />
</p>

<p>
  <%= submit_tag "Join!" %>
</p>
<% end %>
